{
  "pages": [
    "pages/splash/splash",
    "pages/home/home",
    "pages/search/search",
    "pages/profile/profile"
  ],
  "subPackages": [
    {
      "root": "packageStorage/",
      "name": "storage",
      "pages": [
        "pages/box-detail/box-detail",
        "pages/bag-detail/bag-detail",
        "pages/item-detail/item-detail",
        "pages/add-box/add-box",
        "pages/add-bag/add-bag"
      ]
    },
    {
      "root": "packageSearch/",
      "name": "search",
      "pages": [
        "pages/search-result/search-result"
      ]
    },
    {
      "root": "packageCamera/",
      "name": "camera",
      "pages": [
        "pages/camera/camera",
        "pages/item-confirm/item-confirm"
      ]
    },
    {
      "root": "packageProfile/",
      "name": "profile",
      "pages": [
        "pages/settings/settings",
        "pages/about/about",
        "pages/privacy/privacy",
        "pages/help/help"
      ]
    }
  ],
  "preloadRule": {
    "pages/home/home": {
      "network": "all",
      "packages": ["storage"]
    }
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTitleText": "物品管理",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#f8f9fa",
    "enablePullDownRefresh": false,
    "navigationStyle": "custom"
  },
  "tabBar": {
    "custom": false,
    "color": "#8a8a8a",
    "selectedColor": "#1296db",
    "backgroundColor": "#ffffff",
    "borderStyle": "white",
    "list": [
      {
        "pagePath": "pages/home/home",
        "text": "首页",
        "iconPath": "assets/icons/home.png",
        "selectedIconPath": "assets/icons/home-active.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "text": "我的",
        "iconPath": "assets/icons/profile.png",
        "selectedIconPath": "assets/icons/profile-active.png"
      }
    ]
  },

  "requiredBackgroundModes": [],
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}